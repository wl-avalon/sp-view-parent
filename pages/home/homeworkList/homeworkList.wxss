.not-done-homework-area{
  width: 100%;
  border-top: 1rpx solid #dcdcdc;
  border-bottom: 1rpx solid #dcdcdc;
  background-color: #ffffff;
  margin-bottom: 50rpx;
}

.has-done-homework-area{
  width: 100%;
  border-top: 1rpx solid #dcdcdc;
  border-bottom: 1rpx solid #dcdcdc;
  background-color: #ffffff;
}

.not-layout-homework{
  width: 100%;
  height: 90rpx;
  background-color: #ffffff;
  border-top: 1rpx solid #dcdcdc;
  display: flex;
  align-items: center;
  justify-content: center;
}

.not-done-item{
  padding-left: 30rpx;
  border-top: 1rpx solid #dcdcdc;
}

.not-done-title{
  padding-left: 30rpx;
  height: 90rpx;
  display: flex;
  align-items: center; 
  /* justify-content: center; */
}

.has-done-title{
  padding-left: 30rpx;
  height: 90rpx;
  display: flex;
  align-items: center; 
  /* justify-content: center; */
}

.subject-item{
  display: flex;
}

.subject-value{
  position: absolute;
  right: 30rpx;
  height: 90rpx;
  display: flex;
  align-items: center; 
}

.subject-title{
  height: 90rpx;
  display: flex;
  align-items: center; 
}

.content-item{
  display: block;
}

.content-button{
  height: 90rpx;
  display: flex;
  align-items: center;  
  justify-content: center;
}

.has-done-item{
  padding-left: 30rpx;
  border-top: 1rpx solid #dcdcdc;
}

.subject-item-down{
  display: flex;
  background-color: #dcdcdc;
}